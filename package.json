{
  "name": "qcharts-vue",
  "version": "1.0.3",
  "description": "A visulization library based on spritejs",
  "main": "lib/index.js",
  "browser": "lib/index.js",
  "scripts": {
    "start": "npm run dev:demo",
    "dev:demo": "webpack-dev-server --config ./build/webpack.conf.dev.js",
